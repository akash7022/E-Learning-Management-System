<div class="row row-cols-1 row-cols-md-4 g-3 " style="margin-left: 50px; margin-right: 50px;">
    <% for (let lg of lgs) { %>
      <form action="/profile" method="POST" class="row g-3 form-container"  enctype="multipart/form-data">
        <div class="col">
          <div class="card border-secondary mb-3 cuscar">
            <div class="card-body">
              <h5 class="card-title">Assignment</h5>
              <h5 class="card-title">DESIGN AND ANALYSIS OF ALGORITHMS</h5>
              <p class="card-text">Teacher : AM Prasad</p>
              <p class="card-text">Topic &nbsp &nbsp : <%= lg.name%></p>
              <input type="hidden" id="ad" name="ad" value="<%= lg.name%>">
              <a href="<%= lg.link%>" button type="button" class="btn btn-light">View Assignmnet</a>
              <div class="col-auto">
                <label for="note" class="visually-hidden">Password</label>
                <input type="file" class="form-control" name="submitassign" id="note" placeholder="" style="margin-top: 15px; margin-bottom: 15px;">
                <button type="submit" class="btn btn-primary un en">Submit Assignment</button>
            </div>
            </div>
          </div>
        </div>
      </form>

    
    <% } %>
